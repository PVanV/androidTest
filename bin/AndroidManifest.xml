<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.test"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="app.test.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="app.test.Feed"
            android:label="@string/title_activity_feed" >
        </activity>
        <activity
            android:name="app.test.Home"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name="app.test.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="app.test.MenuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="app.test.ProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name="app.test.SearchActivity"
            android:label="@string/title_activity_search" >
        </activity>
        <activity
            android:name="app.test.ListViewAdapter"
            android:label="@string/title_activity_list_view_adapter" >
        </activity>
    </application>

</manifest>